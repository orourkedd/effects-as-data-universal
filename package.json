{
  "name": "effects-as-data-universal",
  "version": "5.0.1",
  "description": "Universal commands for effects-as-data",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/orourkedd/effects-as-data-universal"
  },
  "scripts": {
    "test": "jest",
    "test-mocha":
      "mocha src/**/*.spec.js --recursive --compilers js:babel-register --require babel-polyfill",
    "transpile": "babel src --out-dir es5 --source-maps",
    "deploy":
      "npm run transpile && npm test && npm publish --tag beta && rm -rf es5",
    "prettier": "prettier --write {src/*.js,src/**/*.js}"
  },
  "author": "orourkedd",
  "license": "MIT",
  "dependencies": {
    "effects-as-data": "^3.0.0",
    "fetch-everywhere": "^1.0.5",
    "object-path": "^0.11.4",
    "uuid": "^3.1.0"
  },
  "peerDependencies": {
    "effects-as-data": "^3.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-jest": "^21.0.2",
    "babel-preset-es2015": "^6.24.1",
    "jest-cli": "^21.1.0",
    "object-path-immutable": "^0.5.2",
    "prettier": "^1.6.1",
    "sinon": "^3.2.1"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/test.js",
      "<rootDir>/src/test.js",
      "<rootDir>/es5/test.js"
    ]
  }
}
